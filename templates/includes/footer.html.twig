<footer>
  <div class="footer">
    <div class="container">
      <div class="row">
        <div class="col-12 col-sm-12 col-md-3 footer-col ">
          <section>
            <div class=" align-text-left ">
              <h1 class="h5 mb-4" title="Ansvarlig myndighed"
                  aria-label="Ansvarlig myndighed">Ansvarlig
                myndighed</h1>

              <ul class=" unstyled-list">

                <li class="">
                  <img src="/assets/img/ea507176c85c67b71f7909f3bac14040.png" class="logo" alt="Logostyrelsen" /></li>

              </ul>

            </div>
          </section>
        </div>
        <div class="col-12 col-sm-12 col-md-3 footer-col ">
          <section>
            <div class="align-text-left">

              {% if theme_settings.contact_information.business_owner_name %}
                <div>
                  {{ theme_settings.contact_information.business_owner_name }}
                </div>
              {% endif %}

              {% if theme_settings.contact_information.address %}
                <div>
                  {{ theme_settings.contact_information.address }}
                </div>
              {% endif %}

              {% if theme_settings.contact_information.zipcode and theme_settings.contact_information.city %}
                <div>
                  {{ theme_settings.contact_information.zipcode }} {{ theme_settings.contact_information.city }}
                </div>
              {% endif %}

              <br>

              {% if theme_settings.contact_information.phone_readable %}
                <div>
                  {{ 'Tlf.:' | trans }} {{ theme_settings.contact_information.phone_readable }}
                </div>
              {% endif %}

              {% if theme_settings.contact_information.email %}
                <div>
                  <a href="mailto: {{ theme_settings.contact_information.email }}">
                    {{ theme_settings.contact_information.email }}
                  </a>
                </div>
              {% endif %}

              <br>

              {% if theme_settings.working_hours.working_hours__row_1__day and theme_settings.working_hours.working_hours__row_1__time %}
                <div class="row">
                  <div class="col-6">
                    {{ theme_settings.working_hours.working_hours__row_1__day }}
                  </div>
                  <div class="col-6">
                    <div class="align-text-right">
                      {{ theme_settings.working_hours.working_hours__row_1__time }}
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if theme_settings.working_hours.working_hours__row_2__day and theme_settings.working_hours.working_hours__row_2__time %}
                <div class="row">
                  <div class="col-6">
                    {{ theme_settings.working_hours.working_hours__row_2__day }}
                  </div>
                  <div class="col-6">
                    <div class="align-text-right">
                      {{ theme_settings.working_hours.working_hours__row_2__time }}
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if theme_settings.working_hours.working_hours__row_3__day and theme_settings.working_hours.working_hours__row_3__time %}
                <div class="row">
                  <div class="col-6">
                    {{ theme_settings.working_hours.working_hours__row_3__day }}
                  </div>
                  <div class="col-6">
                    <div class="align-text-right">
                      {{ theme_settings.working_hours.working_hours__row_3__time }}
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if theme_settings.working_hours.working_hours__row_4__day and theme_settings.working_hours.working_hours__row_4__time %}
                <div class="row">
                  <div class="col-6">
                    {{ theme_settings.working_hours.working_hours__row_4__day }}
                  </div>
                  <div class="col-6">
                    <div class="align-text-right">
                      {{ theme_settings.working_hours.working_hours__row_4__time }}
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if theme_settings.working_hours.working_hours__row_5__day and theme_settings.working_hours.working_hours__row_5__time %}
                <div class="row">
                  <div class="col-6">
                    {{ theme_settings.working_hours.working_hours__row_5__day }}
                  </div>
                  <div class="col-6">
                    <div class="align-text-right">
                      {{ theme_settings.working_hours.working_hours__row_5__time }}
                    </div>
                  </div>
                </div>
              {% endif %}

              <br>

              {% if theme_settings.contact_information.additional_information %}
                <div>
                  {{ theme_settings.contact_information.additional_information }}
                </div>
              {% endif %}

              <br>

              <nav role="navigation">
                <div class="flexy-list">

                  {% if theme_settings.social.facebook.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.facebook.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--facebook"
                        title="{{ theme_settings.social.facebook.tooltip }}">
                        <span class="icon fa fa-facebook-f"></span>
                      </a>
                    </div>
                  {% endif %}

                  {% if theme_settings.social.twitter.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.twitter.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--twitter"
                        title="{{ theme_settings.social.twitter.tooltip }}">
                        <span class="icon fa fa-twitter"></span>
                      </a>
                    </div>
                  {% endif %}

                  {% if theme_settings.social.googleplus.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.googleplus.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--google-plus"
                        title="{{ theme_settings.social.googleplus.tooltip }}">
                        <span class="icon fa fa-google-plus"></span>
                      </a>
                    </div>
                  {% endif %}

                  {% if theme_settings.social.linkedin.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.linkedin.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--linkedin"
                        title="{{ theme_settings.social.linkedin.tooltip }}">
                        <span class="icon fa fa-linkedin"></span>
                      </a>
                    </div>
                  {% endif %}

                  {% if theme_settings.social.pinterest.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.pinterest.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--pinterest"
                        title="{{ theme_settings.social.pinterest.tooltip }}">
                        <span class="icon fa fa-pinterest"></span>
                      </a>
                    </div>
                  {% endif %}

                  {% if theme_settings.social.instagram.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.instagram.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--instagram"
                        title="{{ theme_settings.social.instagram.tooltip }}">
                        <span class="icon fa fa-instagram"></span>
                      </a>
                    </div>
                  {% endif %}

                  {% if theme_settings.social.youtube.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.youtube.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--youtube"
                        title="{{ theme_settings.social.youtube.tooltip }}">
                        <span class="icon fa fa-youtube"></span>
                      </a>
                    </div>
                  {% endif %}

                  {% if theme_settings.social.vimeo.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.vimeo.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--vimeo"
                        title="{{ theme_settings.social.vimeo.tooltip }}">
                        <span class="icon fa fa-vimeo"></span>
                      </a>
                    </div>
                  {% endif %}

                  {% if theme_settings.social.github.active %}
                    <div>
                      <a
                        href="{{ theme_settings.social.github.url }}"
                        target="_blank"
                        data-toggle="tooltip"
                        data-placement="top"
                        class="social-icon social-icon--github"
                        title="{{ theme_settings.social.github.tooltip }}">
                        <span class="icon fa fa-github"></span>
                      </a>
                    </div>
                  {% endif %}

                </div>
              </nav>
            </div>
          </section>
        </div>
        <div
          class="col-12 col-sm-12 col-md-3 footer-col d-print-none">
          <section>
            <div class="align-text-left">
              {{ page.footer_first }}
            </div>
          </section>
        </div>
        <div
          class="col-12 col-sm-12 col-md-3 footer-col d-print-none">
          <section>
            <div class="align-text-left">
              {{ page.footer_second }}
            </div>
          </section>
        </div>
      </div>
    </div>
  </div>
</footer>
