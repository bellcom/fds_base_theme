{%
  set classes = [
  'accordion',
  'field',
  'field--name-' ~ field_name|clean_class,
  'field--type-' ~ field_type|clean_class,
  'field--label-' ~ label_display,
]
%}
{%
  set title_classes = [
  'field__label',
  label_display == 'visually_hidden' ? 'visually-hidden',
]
%}

<ul{{ attributes.addClass(classes) }}>
  {% for item in items %}
    <li{{ item.attributes }}>
      {{ item.content }}
    </li>
  {% endfor %}
</ul>
